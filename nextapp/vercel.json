{
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/predict/file",
      "destination": "/api/file_predict"
    },
    {
      "source": "/api/predict",
      "destination": "/api/predict"
    },
    {
      "source": "/api/status",
      "destination": "/api/status"
    },
    {
      "source": "/api/metrics",
      "destination": "/api/metrics"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}